<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.aserbao.aserbaosandroid.ui.customView.countdownView.AboutTimeViewActivity">
    <com.aserbao.aserbaosandroid.AUtils.AUI.layout.FlowLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <Button
            android:id="@+id/start_btn"
            android:text="开始倒计时"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <Button
            android:id="@+id/stop_btn"
            android:text="倒计时停止"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <Button
            android:id="@+id/testViewBtn"
            android:text="检测View生命周期"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

    </com.aserbao.aserbaosandroid.AUtils.AUI.layout.FlowLayout>
    <com.aserbao.aserbaosandroid.ui.customView.countdownView.ACountDownVG
        android:gravity="center"
        android:id="@+id/countDownVG"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <FrameLayout
        android:id="@+id/custom_tainer"
        android:layout_width="wrap_content"
        android:layout_gravity="center"
        android:layout_height="300dp">
        <com.aserbao.aserbaosandroid.ui.customView.countdownView.CountDownView
            android:id="@+id/custom_count_down_view"
            android:layout_gravity="center_horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

    </FrameLayout>



</LinearLayout>
