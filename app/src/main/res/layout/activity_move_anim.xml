<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.aserbao.aserbaosandroid.ui.animation.moveAnimation.MoveAnimActivity">
    <Button
        android:id="@+id/move_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <com.aserbao.aserbaosandroid.ui.animation.moveAnimation.angleShot.MoveView
        android:id="@+id/move_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
    <ImageView
        android:id="@+id/move_anim_iv"
        android:layout_gravity="center"
        android:src="@drawable/emoji_07"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
</FrameLayout>
